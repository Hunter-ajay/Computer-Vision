   "source": [
    "from ultralytics import YOLO\n",
    "import cv2\n",
    "import time\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "49ff622f-0e37-4920-ab2f-959476e10143",
   "metadata": {},
   "outputs": [],
   "source": [
    "model = YOLO(\"yolov8n.pt\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "9dbccc7a-eca9-4ab1-a114-c07d59e8b45d",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "\n",
      "0: 480x640 1 person, 1 chair, 116.7ms\n",
      "Speed: 3.7ms preprocess, 116.7ms inference, 1.9ms postprocess per image at shape (1, 3, 480, 640)\n",
      "\n",
      "0: 480x640 1 person, 61.1ms\n",
      "Speed: 2.1ms preprocess, 61.1ms inference, 1.3ms postprocess per image at shape (1, 3, 480, 640)\n",
     ]
    }
   ],
   "source": [
    "import cv2\n",
    "import time\n",
    "\n",
    "cap = cv2.VideoCapture(0)  # open default webcam\n",
    "\n",
    "if not cap.isOpened():\n",
    "    print(\"Error: Could not open webcam.\")\n",
    "else:\n",
    "    prev_time = 0\n",
    "\n",
    "    while True:\n",
    "        ret, frame = cap.read()\n",
    "        if not ret:\n",
    "            print(\"Failed to capture frame\")\n",
    "            break\n",
    "\n",
    "        # YOLO inference\n",
    "        results = model(frame)\n",
    "        annotated_frame = results[0].plot()\n",
    "\n",
    "        # ----- FPS CALCULATION -----\n",
    "        curr_time = time.time()\n",
    "        fps = 1 / (curr_time - prev_time) if prev_time != 0 else 0\n",
    "        prev_time = curr_time\n",
    "\n",
    "        # Put FPS text on frame\n",
    "        cv2.putText(\n",
    "            annotated_frame,\n",
    "            f\"FPS: {fps:.1f}\",\n",
    "            (10, 30),\n",
    "            cv2.FONT_HERSHEY_SIMPLEX,\n",
    "            1,\n",
    "            (0, 255, 0),\n",
    "            2,\n",
    "            cv2.LINE_AA\n",
    "        )\n",
    "\n",
    "        # Show result\n",
    "        cv2.imshow(\"YOLOv8 Detection - Press Q to exit\", annotated_frame)\n",
    "\n",
    "        if cv2.waitKey(1) & 0xFF == ord('q'):\n",
    "            break\n",
    "\n",
    "cap.release()\n",
    "cv2.destroyAllWindows()\n"
   ]
  },
